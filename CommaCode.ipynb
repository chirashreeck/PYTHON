    "print(\"Name: CHIRASHREE CK\\nUSN: IAY24AI027\\nSection M\")\n",
    "def commaCode(items):\n",
    "    if len(items) == 0:\n",
    "        return ''\n",
    "    elif len(items) == 1:\n",
    "        return items[0]\n",
    "    return ', '.join(items[:-1]) + ' and ' + items[-1]\n",
    "\n",
    "print(commaCode(['apples','bananas','tofu','cats']))"
   ]
